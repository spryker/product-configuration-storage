{
  "name": "spryker/product-configuration-storage",
  "type": "library",
  "description": "ProductConfigurationStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/availability-storage-extension": "^1.0.0",
    "spryker/cart": "^7.8.0",
    "spryker/cart-extension": "^4.0.0",
    "spryker/event-behavior": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/locale": "^3.0.0",
    "spryker/persistent-cart-extension": "^1.0.0",
    "spryker/price-product": "^4.0.0",
    "spryker/price-product-extension": "^1.2.0",
    "spryker/price-product-storage-extension": "^1.2.0",
    "spryker/product-configuration": "^0.1.0",
    "spryker/product-configuration-extension": "^0.1.0",
    "spryker/product-configuration-storage-extension": "^0.1.0",
    "spryker/product-storage": "^1.1.0",
    "spryker/product-storage-extension": "^1.4.0",
    "spryker/propel-orm": "^1.5.0",
    "spryker/publisher-extension": "^1.0.0",
    "spryker/session": "^4.0.0",
    "spryker/storage": "^3.0.0",
    "spryker/synchronization": "^1.9.0",
    "spryker/synchronization-behavior": "^1.0.0",
    "spryker/synchronization-extension": "^1.1.0",
    "spryker/util-encoding": "^2.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/product": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\ProductConfigurationStorage\\Helper\\": "tests/SprykerTest/Zed/ProductConfigurationStorage/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
